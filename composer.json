{
    "name": "bitboxde/minifier",
    "description": "CSS, JS and HTML-Minifier-Plugin for Craft CMS",
    "type": "craft-plugin",
    "version": "4.0.0",
    "keywords": ["craft", "cms", "craftcms", "craft-plugin", "minifier", "css", "js", "seo"],
    "support": {
        "email": "technik@bitbox.de",
        "docs": "https://github.com/bitboxde/minifier/blob/master/docs/README.md",
        "issues": "https://github.com/bitboxde/minifier/issues?q=is%3Aopen",
        "source": "https://github.com/bitboxde/minifier"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "bitbox GmbH & Co. KG",
            "homepage": "https://www.bitbox.de"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0",
        "matthiasmullie/minify": "^1.3",
        "voku/html-min": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "bitboxde\\minifier\\": "src/"
        }
    },
    "extra": {
        "name": "Minifier",
        "handle": "minifier",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/bitboxde/minifier/blob/master/CHANGELOG.md",
        "class": "bitboxde\\minifier\\Minifier"
    }
}
